irmButtonText","cancelButtonText","addressId","type","created"],"sources":["src/views/UserAddress.vue"],"sourcesContent":["<template>\r\n  <div class=\"add-address\">\r\n    <div class=\"title\">我的收货地址</div>\r\n    <el-button type=\"success\" style=\"margin-top:10px;\" @click=\"handleAdd\">添加</el-button>\r\n    <div v-for=\"(item,index) in allAddressData\" :key=\"index\"  class=\"default-address-container\">\r\n      <div>\r\n        <div class=\"address-item\">收货人：{{item.consignee}}</div>\r\n        <div class=\"address-item\">收货地址：{{item.addressDetail}}</div>\r\n        <div class=\"address-item\">收货手机号：{{item.phone}}</div>\r\n      </div>\r\n      <div class=\"right-btn\">\r\n        <el-tag style=\"position:relative;top:-40px;right:-138px;margin-right: 20px;\" v-if=\"item.isDefault\" type=\"success\">默认收货地址</el-tag>\r\n        <!-- <div class=\"marginR20\" @click=\"handleDetail(item)\">详情</div>   -->\r\n        <div class=\"marginR20\" @click=\"handleEdit(item)\">编辑</div>  \r\n        <div class=\"marginR20\" @click=\"handleDel(item)\">删除</div>  \r\n      </div> \r\n    </div>     \r\n    <el-dialog title=\"更换地址\" :visible.sync=\"dialogVisible\" width=\"580px\" :before-close=\"handleClose\">\r\n      <el-table ref=\"multipleTable\" :data=\"allAddressData\" tooltip-effect=\"dark\" style=\"width: 100%\"\r\n       @select=\"handleSelectionChange\">\r\n        <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n        <el-table-column label=\"收货人\" prop=\"consignee\" width=\"120\" show-overflow-tooltip></el-table-column>\r\n        <el-table-column prop=\"addressDetail\" label=\"收获地址\" show-overflow-tooltip></el-table-column>\r\n        <el-table-column prop=\"phone\" label=\"收获手机号\" width=\"120\" show-overflow-tooltip></el-table-column>\r\n      </el-table>\r\n\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"handleClose\">取 消</el-button>\r\n        <el-button type=\"success\" @click=\"submitChange\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n    <el-dialog :title=\"title\" :visible.sync=\"showAdd\" width=\"580px\" :before-close=\"closeAdd\">\r\n      <el-form :model=\"addressData\" ref=\"ruleForm\" label-width=\"100px\" class=\"demo-ruleForm\">\r\n        <el-form-item label=\"收货人：\" prop=\"consignee\">\r\n          <el-input v-model=\"addressData.consignee\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"收货地址：\" prop=\"addressDetail\">\r\n          <el-input v-model=\"addressData.addressDetail\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"收货手机号：\" prop=\"phone\">\r\n          <el-input v-model=\"addressData.phone\"></el-input>\r\n        </el-form-item>\r\n        <el-checkbox style=\"float:right;\" v-model=\"addressData.isDefault\">设为默认地址</el-checkbox>\r\n      </el-form>\r\n\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"closeAdd\">取 消</el-button>\r\n        <el-button type=\"success\" @click=\"updateAddress\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n    \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  defaultAddressInfoUpdate,\r\n  selectDefaultByOwnName,\r\n  addAddress,\r\n  selectAllAddressByUsername,\r\n  deleteAddressById\r\n} from \"../api/address\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      addressData: {\r\n        consignee:\"\",\r\n        phone:'',\r\n        addressDetail:'',\r\n        isDefault:false\r\n      },\r\n      allAddressData:[],\r\n      dialogVisible:false,\r\n      showAdd: false,\r\n      title:'添加地址'\r\n    };\r\n  },\r\n  methods: {\r\n    changeChecked(e) {\r\n      console.log(e);\r\n      this.addressData.isDefault = e;\r\n    },\r\n    getAllAdress(){\r\n      selectAllAddressByUsername().then(res=>{\r\n        console.log(\"data--------------\")\r\n        console.log('ressss',res)\r\n        this.allAddressData = res.data\r\n        \r\n      }).catch(err=>{\r\n        console.log(err)\r\n      })\r\n    },\r\n    handleChange(){\r\n      this.dialogVisible = true\r\n    },\r\n    submitChange(){\r\n      this.dialogVisible = false\r\n    },\r\n    handleClose(){\r\n      this.dialogVisible = false\r\n    },\r\n    handleSelectionChange(row, column){\r\n      this.addressData = [...row]\r\n    },\r\n    handleAdd(){\r\n      this.showAdd = true\r\n      this.title = '添加地址'\r\n    },\r\n    closeAdd(){\r\n      this.showAdd = false\r\n      this.addressData = {}\r\n    },\r\n    updateAddress() {\r\n      if (this.addressData.consignee == \"\") {\r\n        alert(\"收货人不能为空\");\r\n        return;\r\n      } else if (this.addressData.phone == \"\") {\r\n        alert(\"手机号不能为空\");\r\n        return;\r\n      } else if (this.addressData.addressDetail == \"\") {\r\n        alert(\"收货地址不能为空\");\r\n        return;\r\n      }\r\n      if(this.title === '编辑地址'){\r\n        defaultAddressInfoUpdate({\r\n          id: this.addressData.id,\r\n          consignee: this.addressData.consignee,\r\n          phone: this.addressData.phone,\r\n          addressDetail: this.addressData.addressDetail,\r\n          isDefault: this.addressData.isDefault,\r\n        }).then((res) => {\r\n          if (res.flag == true) {\r\n            this.getAllAdress()\r\n            this.$message.success(res.message)\r\n            this.showAdd = false\r\n          } else {\r\n            this.showAdd = false\r\n            this.$message.error(res.data)\r\n          }\r\n        });\r\n      }else{\r\n        addAddress({\r\n          consignee: this.addressData.consignee,\r\n          phone: this.addressData.phone,\r\n          addressDetail: this.addressData.addressDetail,\r\n          isDefault: this.addressData.isDefault,\r\n        }).then((res) => {\r\n          if (res.flag == true) {\r\n            this.getAllAdress()\r\n            this.$message.success(res.message)\r\n            this.showAdd = false\r\n          } else {\r\n            this.showAdd = false\r\n            this.$message.error(res.data)\r\n          }\r\n        });\r\n      }\r\n    },\r\n    handleDetail(item){\r\n\r\n    },\r\n    handleEdit(item){\r\n      this.showAdd = true\r\n      this.title = '编辑地址'\r\n      this.addressData = Object.assign({},{...item})\r\n    },\r\n    handleDel(item){\r\n      this.$confirm('确认删除该信息?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          // type: 'warning'\r\n        }).then(() => {\r\n          deleteAddressById({\r\n            addressId:item.id\r\n          }).then(res=>{\r\n            if(res.flag == true){\r\n              this.getAllAdress()\r\n                this.$message({\r\n                type: 'success',\r\n                message: '删除成功!'\r\n              });\r\n            }else{\r\n              this.$message({type: 'error',message: res.message});\r\n            }\r\n          }).catch(err=>{\r\n            this.$message({\r\n            type: 'error',\r\n            message: '删除失败!'\r\n          });\r\n          })\r\n        }).catch(() => {\r\n          this.$message({\r\n            type: 'info',\r\n            message: '已取消删除'\r\n          });          \r\n        });\r\n    }\r\n  },\r\n  created() {\r\n    this.getAllAdress()\r\n    // selectDefaultByOwnName({})\r\n    //   .then((res) => {\r\n    //     this.addressData = res.data;\r\n    //   })\r\n    //   .catch((err) => {\r\n    //     console.log(err);\r\n    //   });\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.add-address {\r\n  width: 900px;\r\n  float: left;\r\n  padding: 20px;\r\n  background: #fff;\r\n  min-height: 100%;\r\n  .title {\r\n    border-bottom: 1px solid #f2f2f2;\r\n    padding: 10px 0px;\r\n    font-size: 18px;\r\n  }\r\n}\r\n.default-address-container{\r\n  border: 1px dashed #ccc;\r\n  border-radius: 6px;\r\n  padding: 10px 20px;\r\n  display: flex;\r\n  margin-top: 20px;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  .address-item{\r\n    line-height: 30px;\r\n    height: 30px;\r\n    margin-right: 50px;\r\n  }\r\n  .right-btn{\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: flex-start;\r\n    align-items: center;\r\n    .marginR20{\r\n      margin-right: 20px;\r\n      cursor: pointer;\r\n      color: #67C23A;\r\n      &:hover{\r\n        color: #035D1C;\r\n        text-decoration: underline;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"],"mappings":"AAuDA,SACAA,wBAAA,EACAC,sBAAA,EACAC,UAAA,EACAC,0BAAA,EACAC,iBAAA,QACA;AACA;EACAC,KAAA;IACA;MACAC,WAAA;QACAC,SAAA;QACAC,KAAA;QACAC,aAAA;QACAC,SAAA;MACA;MACAC,cAAA;MACAC,aAAA;MACAC,OAAA;MACAC,KAAA;IACA;EACA;EACAC,OAAA;IACAC,cAAAC,CAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,CAAA;MACA,KAAAX,WAAA,CAAAI,SAAA,GAAAO,CAAA;IACA;IACAG,aAAA;MACAjB,0BAAA,GAAAkB,IAAA,CAAAC,GAAA;QACAJ,OAAA,CAAAC,GAAA;QACAD,OAAA,CAAAC,GAAA,WAAAG,GAAA;QACA,KAAAX,cAAA,GAAAW,GAAA,CAAAjB,IAAA;MAEA,GAAAkB,KAAA,CAAAC,GAAA;QACAN,OAAA,CAAAC,GAAA,CAAAK,GAAA;MACA;IACA;IACAC,aAAA;MACA,KAAAb,aAAA;IACA;IACAc,aAAA;MACA,KAAAd,aAAA;IACA;IACAe,YAAA;MACA,KAAAf,aAAA;IACA;IACAgB,sBAAAC,GAAA,EAAAC,MAAA;MACA,KAAAxB,WAAA,OAAAuB,GAAA;IACA;IACAE,UAAA;MACA,KAAAlB,OAAA;MACA,KAAAC,KAAA;IACA;IACAkB,SAAA;MACA,KAAAnB,OAAA;MACA,KAAAP,WAAA;IACA;IACA2B,cAAA;MACA,SAAA3B,WAAA,CAAAC,SAAA;QACA2B,KAAA;QACA;MACA,gBAAA5B,WAAA,CAAAE,KAAA;QACA0B,KAAA;QACA;MACA,gBAAA5B,WAAA,CAAAG,aAAA;QACAyB,KAAA;QACA;MACA;MACA,SAAApB,KAAA;QACAd,wBAAA;UACAmC,EAAA,OAAA7B,WAAA,CAAA6B,EAAA;UACA5B,SAAA,OAAAD,WAAA,CAAAC,SAAA;UACAC,KAAA,OAAAF,WAAA,CAAAE,KAAA;UACAC,aAAA,OAAAH,WAAA,CAAAG,aAAA;UACAC,SAAA,OAAAJ,WAAA,CAAAI;QACA,GAAAW,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAc,IAAA;YACA,KAAAhB,YAAA;YACA,KAAAiB,QAAA,CAAAC,OAAA,CAAAhB,GAAA,CAAAiB,OAAA;YACA,KAAA1B,OAAA;UACA;YACA,KAAAA,OAAA;YACA,KAAAwB,QAAA,CAAAG,KAAA,CAAAlB,GAAA,CAAAjB,IAAA;UACA;QACA;MACA;QACAH,UAAA;UACAK,SAAA,OAAAD,WAAA,CAAAC,SAAA;UACAC,KAAA,OAAAF,WAAA,CAAAE,KAAA;UACAC,aAAA,OAAAH,WAAA,CAAAG,aAAA;UACAC,SAAA,OAAAJ,WAAA,CAAAI;QACA,GAAAW,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAc,IAAA;YAC