-form-item label=\"手机号\">\n        <el-input v-model=\"form.phone\" style=\"width:290px;\"></el-input>\n      </el-form-item>\n      <el-form-item label=\"从事专业\">\n        <el-input v-model=\"form.profession\" style=\"width:290px;\"></el-input>\n      </el-form-item>\n      <el-form-item label=\"职位\">\n        <el-input v-model=\"form.position\" style=\"width:290px;\"></el-input>\n      </el-form-item>\n      <el-form-item label=\"所属单位\">\n        <el-input v-model=\"form.belong\" style=\"width:290px;\"></el-input>\n      </el-form-item>\n    </el-form>\n    <div style=\"text-align:center;margin-right:130px;\">\n      <el-button type=\"success\" @click=\"addExpertInfo\">修改</el-button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { getExpertInfo ,editExpertInfo} from '../api/expert.js'\n\nexport default {\n  data(){\n    return{\n      form:{\n        realName:'',\n        phone:'',\n        profession:'',\n        position:'',\n        belong:''\n      }\n    }\n  },\n  methods:{\n    getExpertData(){\n      getExpertInfo({}).then(res=>{\n        this.form = Object.assign({},{...res.data})\n      }).catch(err=>{\n        console.log(err)\n      })\n    },\n    addExpertInfo(){\n      editExpertInfo(this.form).then(res => {\n        console.log('edit',res)\n        if(res.flag==true){\n          this.$message.success('修改成功')\n        }else{\n          this.$message.error(res.message)\n        }\n      }).catch(err=>{\n        console.log(err)\n      })\n    }\n  },\n  mounted(){\n    this.getExpertData()\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.expert-info-container{\n  width: 900px;\n  height: 100%;\n  background: #fff;\n  padding: 10px 20px;\n}\n</style>"],"mappings":"AA0BA,SAAAA,aAAA,EAAAC,cAAA;AAEA;EACAC,KAAA;IACA;MACAC,IAAA;QACAC,QAAA;QACAC,KAAA;QACAC,UAAA;QACAC,QAAA;QACAC,MAAA;MACA;IACA;EACA;EACAC,OAAA;IACAC,cAAA;MACAV,aAAA,KAAAW,IAAA,CAAAC,GAAA;QACA,KAAAT,IAAA,GAAAU,MAAA,CAAAC,MAAA;UAAA,GAAAF,GAAA,CAAAV;QAAA;MACA,GAAAa,KAAA,CAAAC,GAAA;QAC