: 'post',\n        url: 'cart/add/' + params.order_id,\n        headers: {\n            'Authorization': window.localStorage.token,\n        },\n    })\n}\n\n// 购物车删除商品\nexport function cartDeleteOrder(params) {\n    return request({\n        method: 'delete',\n        url: 'cart/delete/' + params.order_id,\n        headers: {\n            'Authorization': window.localStorage.token,\n        },\n    })\n}\n\n// 展示购物车列表\nexport function cartShow(params) {\n    return request({\n        method: 'get',\n        url: 'cart/show',\n        headers: {\n            'Authorization': window.localStorage.token,\n        },\n    })\n}\n\n// 更新商品数量  /cart/update/{id}/{count}\nexport function updateGoodsCount(params) {\n    return request({\n        method: 'put',\n        url: `/cart/update/${params.id}/${params.count}`,\n        headers: {\n            'Authorization': window.localStorage.token,\n        },\n    })\n}"],"mappings":"AAAA,SAASA,OAAO,QAAQ,kBAAkB;;AAG1C;AACA,OAAO,SAASC,iBAAiBA,CAACC,MAAM,EAAE;EACtC,OAAOF,OAAO,CAAC;IACXG,MAAM,EAAE,MAAM;IACdC,GAAG,EAAE,kBAAkB,GAAGF,MAAM,CAACG,QAAQ;IACzCC,OAAO,EAAE;MACL,eAAe,EAAEC,MAAM,CAACC,YAAY,CAACC;IACzC;EACJ,CAAC,CAAC;AACN;;AAEA;AACA,OAAO,SAASC,WAAWA,CAACR,MAAM,EAAE;EAChC,OAAOF,OAAO,CAAC;IACXG,MAAM,EAAE,KAAK;IACbC,GAAG,EAAE,kBAAkB;IACvBE,OAAO,EAAE;MACL,eAAe,EAAEC,MAAM,CAACC,YAAY,CAACC;IACzC;EACJ,CAAC,CAAC;AACN;;AAEA;AACA,OAAO,SAASE,aAAaA,CAACT,MAAM,EAAE;EAClC,OAAOF,OAAO,CAAC;IACXG,MAAM,EAAE,QAAQ;IAChBC,GAAG,EAAE,qBAAqB,GAAGF,MAAM,CAACG,QAAQ;IAC5CC,OAAO,EAAE;MACL,eAAe,EAAEC,MAAM,CAACC,YAAY,CAACC;IACzC;EACJ,CAAC,CAAC;AACN;;AAIA;AACA,OAAO,SAASG,cAAcA,CAACV,MAAM,EAAE;EACnC,OAAOF,OAAO,CAAC;IACXG,MAAM,EAAE,MAAM;IACdC,GAAG,EAAE,WAAW,GAAGF,MAAM,CAACG,QAAQ;IAClCC,OAAO,EAAE;MACL,eAAe,EAAEC,MAAM,CAACC,YAAY,CAACC;IACzC;EACJ,CAAC,CAAC;AACN;;AAEA;AACA,OAAO,SAASI,eAAeA,CAACX,MAAM,EAAE;EACpC,OAAOF,OAAO,CAAC;IACXG,MAAM,EAAE,QAAQ;IAChBC,GAAG,EAAE,cAAc,GAAGF,MAAM,CAACG,QAAQ;IACrCC,OAAO,EAAE;MACL,eAAe,EAAEC,MAAM,CAACC,YAAY,CAACC;IACzC;EACJ,CAAC,CAA