ounted","commit"],"sources":["src/views/expertQuestion.vue"],"sourcesContent":["<!--专家问答-->\n<template>\n  <div class=\"expert-question-container\">\n    <div v-for=\"(item,index) in questionArray\" :key=\"index\" class=\"question-item\">\n      <div class=\"question-content\">\n        <div class=\"title\">{{item.title}}</div>\n        <div class=\"content\">{{item.question}}</div>\n        <div class=\"item-container\">\n          <div class=\"item-content marginR30\" v-if=\"role==='expert'\">提问者：<span class=\"item-text\">{{item.questioner}}</span> </div>\n          <div class=\"item-content marginR30\" v-if=\"role==='expert'\">提问者联系方式：<span class=\"item-text\">{{item.phone}}</span> </div>\n          <div class=\"item-content marginR30\" v-if=\"role==='questioner'\">专家姓名：<span class=\"item-text\">{{item.expertName}}</span></div>\n          <el-tag :type=\"item.status === 0 ? 'danger':'success'\" size=\"mini\">{{item.status === 0 ? '未回答' :'已回答'}}</el-tag>\n        </div>\n      </div>\n      <div class=\"question-btn\">\n        <div class=\"btn-text\" @click=\"handleDetail(item)\">详情</div>\n        <div class=\"btn-text\" @click=\"handleEdit(item)\" v-if=\"role==='questioner'\">修改</div>\n        <div class=\"btn-text\" @click=\"handleEdit(item)\" v-if=\"role==='expert'\">回答</div>\n        <div class=\"btn-text\" @click=\"delQuestion(item)\">删除</div>\n      </div>\n    </div>\n    <el-dialog title=\"详情\" :visible.sync=\"showDetail\" width=\"600px\" :before-close=\"detailClose\">\n      <div>\n        <div class=\"detail-item\">\n          <div class=\"item-title\">问题标题：</div>\n          <div class=\"item-content\">{{detailObj.title}}</div>\n        </div>\n        <div class=\"detail-item\">\n          <div class=\"item-title\">问题内容：</div>\n          <div class=\"item-content\">{{detailObj.question}}</div>\n        </div>\n        <div class=\"detail-item\" v-if=\"role==='questioner'\">\n          <div class=\"item-title\">专家姓名：</div>\n          <div class=\"item-content\">{{detailObj.expertName}}</div>\n        </div>\n        <!-- <div class=\"detail-item\">\n          <div class=\"item-title\">提问者：</div>\n          <div class=\"item-content\">{{detailObj.questioner}}</div>\n        </div> -->\n        <div class=\"detail-item\">\n          <div class=\"item-title\">问题状态：</div>\n          <el-tag type=\"danger\" size=\"mini\">{{detailObj.status === 0 ? '未回答' :'已回答'}}</el-tag>\n        </div>\n      </div>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <!-- <el-button @click=\"showDetail = false\">取 消</el-button> -->\n        <el-button type=\"primary\" @click=\"showDetail = false\">确 定</el-button>\n      </span>\n    </el-dialog>\n    <el-dialog :title=\"title\" :visible.sync=\"dialogVisible\" width=\"650px\" :before-close=\"closeRevise\">\n      <div>\n        <div class=\"detail-item\">\n          <div class=\"item-title\">问题标题：</div>\n          <div class=\"item-content\">{{detailObj.title}}</div>\n        </div>\n        <!-- 专家不能改问题内容 -->\n        <div class=\"detail-item\" v-if=\"role==='expert'||detailObj.status === 1\">\n          <div class=\"item-title\">内容：</div>\n          <div class=\"item-content\">{{detailObj.question}}</div>\n        </div>\n        <!-- 提问者可以改问题内容 -->\n        <el-form ref=\"form\" :model=\"detailObj\" label-width=\"60px\" v-if=\"role==='questioner'&&detailObj.status === 0\">\n          <el-form-item label=\"内容：\">\n            <el-input type=\"textarea\" v-model=\"detailObj.question\"></el-input>\n          </el-form-item>\n        </el-form>\n\n        <div class=\"detail-item\" v-if=\"role==='questioner'\">\n          <div class=\"item-title\">回答：</div>\n          <div class=\"item-content\">{{detailObj.answer}}</div>\n        </div>\n        <el-form ref=\"form\" :model=\"detailObj\" label-width=\"60px\" v-if=\"role==='expert'\">\n          <el-form-item label=\"回答：\">\n            <el-input type=\"textarea\" v-model=\"detailObj.answer\"></el-input>\n          </el-form-item>\n        </el-form>\n      </div>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"closeRevise\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"submitRevise\">确 定</el-button>\n      </span>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport { selectQuestionByUser,reviseQuestionByUserId,delQuestionByUserId } from '../api/question.js'\n\nexport default {\n  data(){\n    return{\n      questionArray:[],\n      showDetail: false,\n      dialogVisible: false,\n      role:\"\",\n      title:\"\",\n      detailObj:{\n        title:'',\n        question:'',\n        expertName:'',\n        answer:'',\n        address:'',\n        area:'',\n        expertName:'',\n        plantCondition:'',\n        plantDetail:'',\n        plantName:'',\n        questioner:'',\n        soilCondition:'',\n        status:1\n      }\n    }\n  },\n  methods:{\n    getData(){\n      this.role = this.$store.getters.isExpert?'expert':'questioner'\n      console.log('role',this.role)\n      // kind：普通用户：questioner；专家：expert\n      selectQuestionByUser({role:this.role}).then(res => {\n        console.log('rererer',res)\n        this.questionArray = res.data\n      }).catch(err=>{\n        console.log(err)\n      })\n    },\n    delQuestion(item){\n      this.$confirm('确认删除该行信息？', '删除', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        delQuestionByUserId({id:item.id}).then(res=>{\n          this.$message({\n            type: 'success',\n            message: '删除成功!'\n          });\n          this.getData()\n        }).catch(err=>{\n          console.log(err)\n        })\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: '已取消删除'\n        });          \n      });\n    },\n    handleDetail(item){\n      this.showDetail = true\n      this.detailObj = Object.assign({},{...item})\n    },\n    detailClose(){\n      this.showDetail = false\n    },\n    handleEdit(item){\n      if(this.role === 'questioner'){\n        this.title = \"修改\";\n      }else{\n        this.title = \"回答\";\n      }\n      this.dialogVisible = true\n      this.detailObj = Object.assign({},{...item})\n    },\n    closeRevise(){\n      this.dialogVisible = false\n    },\n    submitRevise(){\n      if(this.role === 'expert'){\n        this.detailObj.status = 1\n      }else{\n        this.detailObj.status = 0\n      }\n      \n      reviseQuestionByUserId(this.detailObj).then(res => {\n        this.$message.success('修改成功！')\n        this.dialogVisible = false\n        this.getData()\n      }).catch(err=>{\n        console.log(err)\n      })\n    }\n  },\n  mounted(){\n    this.$store.commit(\"updateUserActiveIndex\", \"4-1\");\n    this.getData()\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.expert-question-container{\n  width: 900px;\n  min-height: 100%;\n  // padding: 10px 20px;\n  background: #fff;\n  .question-item{\n    border:1px solid #f2f2f2;\n    border-radius: 6px;\n    padding: 10px 20px;\n    margin: 10px 20px;\n    display: flex;\n    flex-direction: row;\n    justify-content: space-between;\n    align-items: center;\n    .question-content{\n      width: 650px;\n      .title{\n        font-weight: bold;\n        font-size: 18px;\n        line-height: 30px;\n        height: 30px;\n      }\n      .content{\n        line-height: 25px;\n        height: auto;\n        word-break: break-all;\n      }\n      .item-container{\n        display: flex;\n        flex-direction: row;\n        justify-content: flex-start;\n        align-items: center;\n        font-size: 12px;\n        line-height: 20px;\n      }\n      .marginR30{\n        margin-right: 30px;\n      }\n      .item-text{\n        color: #666;\n      }\n    }\n    .question-btn{\n      display: flex;\n      .btn-text{\n        height: 25px;\n        cursor: pointer;\n        margin-right: 10px;\n        color: #67C23A;\n        &:hover{\n          color: #035D1C;\n          text-decoration: underline;\n        }\n      }\n    }\n  }\n  .detail-item{\n    display: flex;\n    line-height: 30px;\n    .item-content{\n      line-height: 30px;\n      height: auto;\n      width: 480px;\n    }\n    .item-title{\n      width: 80px;\n      height: 30px;\n      font-weight: bold;\n      color: #333;\n    }\n  }\n}\n</style>"],"mappings":"AAsFA,SAAAA,oBAAA,EAAAC,sBAAA,EAAAC,mBAAA;AAEA;EACAC,KAAA;IACA;MACAC,aAAA;MACAC,UAAA;MACAC,aAAA;MACAC,IAAA;MACAC,KAAA;MACAC,SAAA;QACAD,KAAA;QACAE,QAAA;QACAC,UAAA;QACAC,MAAA;QACAC,OAAA;QACAC,IAAA;QACAH,UAAA;QACAI,cAAA;QACAC,WAAA;QACAC,SAAA;QACAC,UAAA;QACAC,aAAA;QACAC,MAAA;MACA;IACA;EACA;EACAC,OAAA;IACAC,QAAA;MACA,KAAAf,IAAA,QAAAgB,MAAA,CAAAC,OAAA,CAAAC,QAAA;MACAC,OAAA,CAAAC,GAAA,cAAApB,IAAA;MACA;MACAP,oBAAA;QAAAO,IAAA,OAAAA;MAAA,GAAAqB,IAAA,CAAAC,GAAA;QACAH,OAAA,CAAAC,GAAA,YAAAE,GAAA;QACA,KAAAzB,aAAA,GAAAyB,GAAA,CAAA1B,IAAA;MACA,GAAA2B,KAAA,CAAAC,GAAA;QACAL,OAAA,CAAAC,GAAA,CAAAI,GAAA;MACA;IACA;IACAC,YAAAC,IAAA;MACA,KAAAC,QAAA;QACAC,iBAAA;QACAC,gBAAA;QACAC,IAAA;MACA,GAAAT,IAAA;QACA1B,mBAAA;UAAAoC,EAAA,EAAAL,IAAA,CAAAK;QAAA,GAAAV,IAAA,CAAAC,GAAA;UACA,KAAAU,QAAA;YACAF,IAAA;YACAG,OAAA;UACA;UACA,KAAAlB,OAAA;QACA,GAAAQ,KAAA,CAAAC,GAAA;UACAL,OAAA,CAAAC,GAAA,CAAAI,GAAA;QACA;MACA,GAAAD,KAAA;QACA,KAAAS,QAAA;UACAF,IAAA;UACAG,OAAA;QACA;MACA;IACA;IACAC,aAAAR,IAAA;MACA,KAAA5B,UAAA;MACA,KAAAI,SAAA,GAAAiC,MAAA,CAAAC,MAAA;QAAA,GAAAV;MAAA;IACA;IACAW,YAAA;MACA,KAAAvC,UAAA;IACA;IACAwC,WAAAZ,IAAA;MACA,SAAA1B,IAAA;QACA,KAAAC,KAAA;MACA;QACA,KAAAA,KAAA;MACA;MACA,KAAAF,aAAA;MACA,KAAAG,SAAA,GAAAiC,MAAA,CAAAC,MAAA;QAAA,GAAAV;MAAA;IACA;IACAa,YAAA;MACA,KAAAxC,aAAA;IACA;IACAyC,aAAA;MACA,SAAAxC,IAAA;QACA,KAAAE,SAAA,CAAAW,MAAA;MACA;QACA,KAAAX,SAAA,CAAAW,MAAA;MACA;MAEAnB,sBAAA,MAAAQ,SAAA,EAAAmB,IAAA,CAAAC,GAAA;QACA,KAAAU,QAAA,CAAAS,OAAA;QACA,KAAA1C,aAAA;QACA,KAAAgB,OAAA;MACA,GAAAQ,KAAA,CAAAC,GAAA;QACAL,OAAA,CAAAC,GAAA,CAAAI,GAAA;MACA;IACA;EACA;EACAkB,QAAA;IACA,KAAA1B,MAAA,CAAA2B,MAAA;IACA,KAAA5B,OAAA;EACA;AACA","ignore